<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:controller="org.fellesprosjekt.gruppe24.client.controllers.InvitationController" 
prefHeight="461.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fellesprosjekt.gruppe24.client.controllers.InvitationController">
   <children>
      <Label layoutX="23.0" layoutY="21.0" text="Møteinvitasjon" AnchorPane.leftAnchor="23.0">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="37.0" layoutY="72.0" prefHeight="301.0" prefWidth="287.0" AnchorPane.leftAnchor="37.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Beskrivelse:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Sted:" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Start:" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Slutt:" GridPane.rowIndex="3">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Deltagere:" GridPane.rowIndex="4">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Møteleder:" GridPane.rowIndex="5">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label text="Status:" GridPane.rowIndex="8">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <!-- beskrivning -->
            <TextArea fx:id="decriptionField" disable="true" editable="false" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <!-- rum -->
            <TextField fx:id="roomField" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <!-- starttid -->
            <TextField fx:id="startField" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <!-- sluttid -->
            <TextField fx:id="endField" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <!-- möteledare -->
            <TextField fx:id="leaderField" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <!-- deltagare -->
            <ListView fx:id="participantField" disable="true" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <RadioButton mnemonicParsing="false" text="Aksepter" GridPane.columnIndex="1" GridPane.rowIndex="7">
               <font>
                  <Font size="14.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="gruppe" />
               </toggleGroup>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="Avvis" toggleGroup="$gruppe" GridPane.columnIndex="1" GridPane.rowIndex="8">
               <font>
                  <Font size="14.0" />
               </font>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="Venter" toggleGroup="$gruppe" GridPane.columnIndex="1" GridPane.rowIndex="9">
               <font>
                  <Font size="14.0" />
               </font>
            </RadioButton>
         </children>
      </GridPane>
      <Button onAction="#pressOkButton" layoutX="23.0" layoutY="396.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" text="OK">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button onAction="#pressAvbrytButton" layoutX="126.0" layoutY="396.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" text="Avbryt">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button onAction="#pressSkjulButton" layoutX="231.0" layoutY="396.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="99.0" text="Skjul" AnchorPane.leftAnchor="229.0">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
